        <activity android:name="com.cookandroid.cartoon.ui.home.HomeFragment" />
        <activity android:name="com.cookandroid.cartoon.ui.notifications.NotificationsFragment" />

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.cookandroid.cartoon.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>