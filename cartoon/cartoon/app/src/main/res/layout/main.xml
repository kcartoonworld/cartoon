<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFFFFF"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        >
        <ImageView
            android:id="@+id/main_logo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#00000000"
            android:src="@drawable/main_logo"
            android:scaleType="centerCrop">
        </ImageView>
    </LinearLayout>
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center">
        <TableRow android:gravity="center">
        </TableRow>
        <TableRow android:gravity="center">
            <Button
                android:id="@+id/cartoon"
                android:layout_width="210dp"
                android:onClick="Numbering"
                android:background="#FFFFFF"
                android:textSize="25dp"
                android:text="만화" />
            <Button
                android:id="@+id/ranking"
                android:layout_width="220dp"
                android:background="#FFFFFF"
                android:textSize="25dp"
                android:text="사용자 랭킹"/>
        </TableRow>
    </TableLayout>
    <ScrollView
        android:id="@+id/viewContentOne"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent">
        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center">
            <TableRow android:gravity="center">
                <ImageButton
                    android:id="@+id/fisrt"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/first"
                    android:scaleType="centerCrop">
                </ImageButton>
                <ImageButton
                    android:id="@+id/second"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/second"
                    android:scaleType="centerCrop">
                </ImageButton>
                <ImageButton
                    android:id="@+id/third"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/third"
                    android:scaleType="centerCrop">
                </ImageButton>
            </TableRow>
            <TableRow android:gravity="center">
                <TextView
                    android:id="@+id/first_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/second_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/third_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
            </TableRow>
            <TableRow android:gravity="center">
                <ImageButton
                    android:id="@+id/fourth"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/fourth"
                    android:scaleType="centerCrop">
                </ImageButton>
                <ImageButton
                    android:id="@+id/fifth"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/fifth"
                    android:scaleType="centerCrop">
                </ImageButton>
                <ImageButton
                    android:id="@+id/sixth"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/sixth"
                    android:scaleType="centerCrop">
                </ImageButton>
            </TableRow>
            <TableRow android:gravity="center">
                <TextView
                    android:id="@+id/fouth_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/fifth_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/sixth_text"
                    android:layout_width="138dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
            </TableRow>

            <TableRow android:gravity="center">
                <ImageButton
                    android:id="@+id/seventh"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:scaleType="centerCrop"
                    android:src="@drawable/seventh"></ImageButton>
                <ImageButton
                    android:id="@+id/eghith"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/eghith"
                    android:scaleType="centerCrop">
                </ImageButton>
                <ImageButton
                    android:id="@+id/nineth"
                    android:layout_width="138dp"
                    android:layout_height="150dp"
                    android:background="#00000000"
                    android:src="@drawable/nineth"
                    android:scaleType="centerCrop">
                </ImageButton>
            </TableRow>
            <TableRow android:gravity="center">
                <TextView
                    android:id="@+id/seventh_text"
                    android:layout_width="137dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/eghith_text"
                    android:layout_width="137dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/nineth_text"
                    android:layout_width="137dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="만화제목 \n 추천수: "
                    android:textColor="#100719"
                    android:textSize="20dp" />
            </TableRow>
            <TableRow android:gravity="center">
                <ImageButton
                    android:id="@+id/more"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_width="138dp"
                    android:layout_height="40dp"
                    android:background="#00000000"
                    android:src="@drawable/more"
                    android:scaleType="centerCrop">
                </ImageButton>
            </TableRow>
        </TableLayout>
    </ScrollView>
</LinearLayout>